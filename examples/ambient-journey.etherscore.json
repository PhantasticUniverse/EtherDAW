    "buildup": {
      "bars": 8,
      "tracks": {
        "pad": { "pattern": "pad_main", "repeat": 2, "velocity": 0.5 },
        "bass": { "pattern": "bass_pulse", "velocity": 0.5, "humanize": 0.05 }
      }
    },
    "main_a": {
      "bars": 16,
      "tracks": {
        "pad": { "pattern": "pad_main", "repeat": 4, "velocity": 0.6 },
        "bass": { "pattern": "bass_movement", "repeat": 4, "velocity": 0.65, "humanize": 0.1 },
        "lead": { "pattern": "lead_melody", "repeat": 2, "velocity": 0.55, "humanize": 0.15 }
      }
    },
    "main_b": {
      "bars": 16,
      "tracks": {
        "pad": { "pattern": "pad_main", "repeat": 4, "velocity": 0.6 },
        "bass": { "pattern": "bass_movement", "repeat": 4, "velocity": 0.65, "humanize": 0.1 },
        "lead": { "pattern": "lead_variation", "repeat": 2, "velocity": 0.55, "humanize": 0.15 },
