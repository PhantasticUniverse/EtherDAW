    "finale": {
      "bars": 16,
      "tracks": {
        "pad": { "pattern": "pad_main", "repeat": 4, "velocity": 0.7 },
        "bass": { "pattern": "bass_movement", "repeat": 4, "velocity": 0.7, "humanize": 0.1 },
        "lead": { "pattern": "lead_melody", "velocity": 0.6, "humanize": 0.1 },
        "bells": { "pattern": "bells_shimmer", "repeat": 8, "velocity": 0.35 }
      }
